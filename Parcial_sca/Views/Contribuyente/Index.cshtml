@model IEnumerable<Parcial_sca.Models.Contribuyente>
@{

}
<p>
    <a asp-action="AddC"><input type="submit" value="Agregar"/></a>
</p>
<table>
    <thead>
        <tr>
            <td>
                @Html.DisplayNameFor(model => model.CIva)
            </td>
            <td>
                @Html.DisplayNameFor(model => model.Cuit)
            </td>
            <td>
                @Html.DisplayNameFor(model => model.RZ)
            </td>
            <td>
                @Html.DisplayNameFor(model => model.Domicilio)
